{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-ide-helper": "^2.4",
        "bizly/roles": "^2.1",
        "christofferok/laravel-emojione": "^2.1",
        "emojione/assets": "^3.1",
        "erusev/parsedown": "^1.6",
        "germey/geetest": "^3.0",
        "guzzlehttp/guzzle": "^6.2",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelchen/laravel-editormd": "^2.5",
        "naux/sendcloud": "^1.1",
        "overtrue/flysystem-qiniu": "^0.0.2",
        "overtrue/laravel-emoji": "@dev",
        "overtrue/laravel-filesystem-qiniu": "^0.0.2",
        "overtrue/laravel-lang": "~3.0",
        "overtrue/laravel-ueditor": "~1.0",
        "overtrue/socialite": "~1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
